<% @today = Date.today.to_s %><% @LineCounter = 1 %><% @empty %><% headers = ['INVOICE','LINE','ITEM','DESCRIPTION','SELL BY DATE','CW','QUANTITY','UOM','PRICE','PRICE UOM'] %><%= CSV.generate_line headers %><% @inventorysession.detail.each do |detail| %><% if detail.count_type == 'Case' %><% @UOM = 'CS' %><% else %><% @UOM = 'PC' %><% end %><%= CSV.generate_line([0,@LineCounter,detail.bhproduct_number, Product.find_by(bhproduct_number: detail.bhproduct_number).name,@today,'EDD', detail.quantity, @UOM,@empty,@empty]) %><% @LineCounter = @LineCounter + 1 %><% end %>